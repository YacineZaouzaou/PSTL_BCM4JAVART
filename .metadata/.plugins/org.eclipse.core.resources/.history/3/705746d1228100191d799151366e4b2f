package fr.upmc.pstl;
import java.util.List;
import java.util.Map;

import fr.upmc.pstl.annotations.CyclePeriod;

@CyclePeriod(period = 15)
public class ComponentRT extends AbstractComponentRT {
	
	public ComponentRT(String uri, Map<String , Object> vars, List<AbstractTask> tasks) {
		super(uri, vars, tasks);
		super.scheduler(this);
	}
}



